<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="SchoolV.EditStudent">
    <ContentPage.Content>
        <StackLayout>
            <Entry x:Name="nombre" Placeholder="Nombre"/>
            <Entry x:Name="paterno" Placeholder="Apellido Paterno"/>
            <Entry x:Name="materno" Placeholder="Apellido Materno"/>
            <Button Text="Add" Clicked="ConfirmStudent"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>